                            android:layout_marginStart="8dp"
                            android:layout_toEndOf="@+id/image_ciudad"
                            android:layout_toRightOf="@+id/image_ciudad"
                            android:textSize="@dimen/dimen_letra_app_estandar" />

                    </RelativeLayout>

                    <RelativeLayout
